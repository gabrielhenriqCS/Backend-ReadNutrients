generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model consults {
  id        String     @id
  barcode   String     @unique(map: "Consults_barcode_key")
  date      DateTime   @default(now())
  createdAt DateTime   @default(now())
  nutrition nutrition?

  @@index([barcode], map: "Consults_barcode_idx")
}

model nutrition {
  id           String   @id
  barcode      String
  calorias     Float
  carboidratos Float
  proteinas    Float
  gorduras     Float
  fibras       Float
  consultsId   String   @unique(map: "Nutrition_consultsId_key")
  createdAt    DateTime @default(now())
  updatedAt    DateTime
  consults     consults @relation(fields: [consultsId], references: [id], onDelete: Cascade, map: "Nutrition_consultsId_fkey")

  @@index([barcode], map: "Nutrition_barcode_idx")
}
